@{
    ViewData["Title"] = "Project Page";
}

<div>
    <h3>Create project</h3>
</div>

<container>
    <row>
        <form method="post">
            <div>
                <label class="col-md-1" for="title">
                    Title:
                </label>
                <input class="col-md-2" type="text" id="title" name="title" />
            </div>
            <div>
                @foreach (var user in ViewBag.Users)
                {
                    <input name="AreChecked" type="checkbox" value="@user.Id" /> @user.UserName

                    <br />
                }
               
            </div>
                
            
            <div>
                <input type="submit" value="submit" />
            </div>
        </form>
    </row>
</container>

